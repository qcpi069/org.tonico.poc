<!-- COPIES DATA FROM IRON DMA.t_rxclaim_plan_temp TO GDXPRD VRAP.trxclaim_plan_temp  -->
<!-- THE SCHEDULE THAT EXECUTES THIS LOADS Saaturday AND IS RUN Sunday dependent upon EDW jobs - see maestro list -->
<!-- If ABEND, the job can be restarted from beginning with no issues. -->


<sqml>
	<property name="TIMEOUT" value="0"/>
	<datasources resource="datasource.xml"/>
	
	<!--
		Set-up a try block to send an email on exception.	
	-->         
	<command type="try">
		<try>
			<!-- Clear out the table on GDXPRD. -->
			<command type="run" datasource="gdxprd" transaction="false">
				<![CDATA[
					DELETE FROM VRAP.trxclaim_plan_temp
				]]>
			</command>
			<!-- Copy data from IRON to GDXPRD -->
				<command type="copy" batch="500">
					<source type="sql" datasource="iron"><![CDATA[
					SELECT a.clnt_id
					      ,a.carr_id
					      ,a.acct_id
					      ,a.grp_id
					      ,a.plan_id
					      ,a.plan_eff_date
					      ,a.plan_status
					      ,a.prfrd_brand_copay_pct
					      ,a.prfrd_brand_copay_amt
					      ,a.prfrd_brand_copay_min
					      ,a.prfrd_brand_copay_max
					      ,a.prfrd_brand_copay_type
					      ,a.non_prfrd_brand_copay_pct
					      ,a.non_prfrd_brand_copay_amt
					      ,a.non_prfrd_brand_copay_min
					      ,a.non_prfrd_brand_copay_max
					      ,a.non_prfrd_brand_copay_type
					      ,a.copay_sprd_amt
					      ,a.copay_sprd_min_amt
					      ,a.copay_sprd_max_amt
					      ,a.copay_sprd_pct
					      ,a.prfrd_drug_list_flag
					      ,a.invtn_flag
					      ,a.algn_lvl_gid
					      ,a.lcm_code
					  FROM dma.t_rxclaim_plan_temp a
				]]></source>
                                <target type="queue" threadCount="5">
				<target
					type             = "sql"
					datasource       = "gdxprd"
					transaction      = "false"
					batch            = "500"
					reportInterval   = "500"
					queryTimeout     = "${TIMEOUT}"
					><![CDATA[
					     INSERT INTO vrap.trxclaim_plan_temp
						(clnt_id
						,carr_id
						,acct_id 
						,grp_id 
						,plan_id
						,plan_eff_date
						,plan_status 
						,prfrd_brand_copay_pct
						,prfrd_brand_copay_amt
						,prfrd_brand_copay_min
						,prfrd_brand_copay_max
						,prfrd_brand_copay_type
						,non_prfrd_brand_copay_pct
						,non_prfrd_brand_copay_amt
						,non_prfrd_brand_copay_min
						,non_prfrd_brand_copay_max
						,non_prfrd_brand_copay_type
						,copay_sprd_amt
						,copay_sprd_min_amt
						,copay_sprd_max_amt
						,copay_sprd_pct
						,prfrd_drug_list_flag
						,invtn_flag
						,algn_lvl_gid
						,lcm_code )
						VALUES ( ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?  )
					]]></target>
                                   </target>
			</command>				 
		</try>
		<catch>
			<!-- Send an email on error -->
			<command
				type="mail"
				mailTo="gdxitd@caremark.com"
				mailFrom="TRXCLAIM_PLAN_TEMP@caremark.com"
				subject="[ERROR] GDX_GD_Extract_and_Load_TRXCLAIM_PLAN_TEMP.xml"
				contentType="text/html"
				throwsException="false"
			><![CDATA[
				<html>
				<head>
				<title>[ERROR] GDX_GD_Extract_and_Load_TRXCLAIM_PLAN_TEMP.xml</title>
				</head>
				<body>
			
				<h3><u>[ERROR] GDX_GD_Extract_and_Load_TRXCLAIM_PLAN_TEMP.xml</u></h3>
			
				<table cellpadding="2">
					<tr><td><b>Host:</b></td><td>${localhost}<td></tr>
					<tr><td><b>Started:</b></td><td>${startTime}<td></tr>
					<tr><td><b>Script:</b></td><td>${scriptName}<td></tr>
					#if (${system.getProperty('logfile')})
						<tr>
							<td><b>Log File:</b></td>
							<td>
								${system.getProperty('logfile')}
							<td>
						</tr>
					#end
				</table>

				<br>			
				<br>
				
				#if (${script.getLastStatement()})
					<h3><u>Last Statement</u></h3>
					<font style="color: blue; font-size: small">	
					${script.getLastStatement()}
					</font>
				
					<br>
					<br>
				#end
			
				<h3><u>Stack Trace</u></h3>		
				<pre style="color: red; font-size: small;">${stackTrace}</pre>
				</body>
				</html>
			]]></command>
		</catch>
	</command>
</sqml>
