<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 <!-- 
 	 *   Date			Author		Project		  Description
	 ************       ******      *******       ************
	 * 04/13/2010		qcpi08a		cashRecon     add PRCS_APPL_ID
 -->
 
<hibernate-mapping>
  <class name="com.caremark.controller.beans.ProcessControl" table="VRAP.RCIT_PRCS_CNTL">
   <id name="processID" type="int" column="PRCS_GID" >
   		<generator class="native"/>
  </id>

  <property name="processCode">
     <column name="PRCS_CD" />
  </property>
  <property name="taskSequenceType">
    <column name="TASK_SEQ_TYP_CD"/>
  </property>
  <property name="statusCode">
    <column name="PRCS_STAT_CD"/>
  </property>
  <property name="requesterID">
     <column name="RQSTR_ID" />
  </property>
  <property name="insertDate">
     <column name="INSRT_TS" />
  </property>
  <property name="updateUserID">
    <column name="UPDT_USER_ID"/>
  </property>
  <property name="updateDate">
    <column name="UPDT_TS"/>
  </property>
  <property name="startTime">
     <column name="STRT_TS" />
  </property>
  <property name="endTime">
    <column name="END_TS"/>
  </property>
  <property name="prefixValue">
    <column name="TBL_PRFX_TXT"/>
  </property>
  <set name="processcontrolDetailSet" lazy="false" cascade="all" inverse="false" order-by="SEQ_NB asc">
  	<key column="PRCS_GID"></key>
  	<one-to-many class="com.caremark.controller.beans.ProcessControlDetail"/>
  </set>
  
  <!-- Added for BSR 47215 - Start Change -->
  <property name="deferredTs">
  	<column name="DFER_STRT_TS"/>
  </property>
  <!-- Added for BSR 47215 - End Change -->
   <property name="prcsApplId">
  	<column name="PRCS_APPL_ID"/>
  </property>  
</class>
</hibernate-mapping>
