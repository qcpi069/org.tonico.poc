<?xml version="1.0" encoding="ISO-8859-1"?>

<project    default="GDX:build-all"
            xmlns:j="jelly:core"
            xmlns:maven="jelly:maven">

    <preGoal name="GDX:build-all">
		<attainGoal name="dim:fetch"/>
    </preGoal>
	<goal name="GDX:build-all">
	    <maven:reactor  basedir="${dim.work.dir}"
						includes="*/project.xml"
						goals="GDXDist"
						banner="Building Rebates GDX Application"
						ignoreFailures="false"/>
	</goal>
</project>