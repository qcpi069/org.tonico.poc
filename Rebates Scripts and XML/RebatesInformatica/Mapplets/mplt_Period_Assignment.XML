        <TRANSFORMATION DESCRIPTION ="" NAME ="EXP_Logic" OBJECTVERSION ="1" REUSABLE ="NO" TYPE ="Expression" VERSIONNUMBER ="1">
            <TRANSFORMFIELD DATATYPE ="date/time" DEFAULTVALUE ="" DESCRIPTION ="" NAME ="in_PERIOD_END_DT" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="29" SCALE ="9"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="" NAME ="in_PRD_ID" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="6" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="" NAME ="in_NRE_CLM_CD" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="10" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="" EXPRESSION ="IIF(in_NRE_CLM_CD = &apos;0&apos; OR in_NRE_CLM_CD = &apos;2&apos;, &apos;Y&apos;, &apos;N&apos;)" EXPRESSIONTYPE ="GENERAL" NAME ="v_NRE" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="1" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="" EXPRESSION ="IIF(v_NRE = &apos;N&apos; OR ISNULL(in_PRD_ID) , &apos;HOLD&apos;, in_PRD_ID)" EXPRESSIONTYPE ="GENERAL" NAME ="v_PRD_ID" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="6" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="date/time" DEFAULTVALUE ="" DESCRIPTION ="" EXPRESSION ="IIF(v_NRE = &apos;N&apos; OR ISNULL(in_PERIOD_END_DT), TO_DATE(&apos;12-31-9999&apos;, &apos;MM-DD-YYYY&apos;), in_PERIOD_END_DT)" EXPRESSIONTYPE ="GENERAL" NAME ="v_PERIOD_END_DT" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="29" SCALE ="9"/>
            <TRANSFORMFIELD DATATYPE ="date/time" DEFAULTVALUE ="" DESCRIPTION ="" EXPRESSION ="IIF(RTRIM(v_PRD_ID)=&apos;HOLD&apos;,SYSDATE,NULL)" EXPRESSIONTYPE ="GENERAL" NAME ="v_HOLD_QUE_DT" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="29" SCALE ="9"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="ERROR(&apos;transformation error&apos;)" DESCRIPTION ="" EXPRESSION ="v_PRD_ID" EXPRESSIONTYPE ="GENERAL" NAME ="PRD_ID" PICTURETEXT ="" PORTTYPE ="OUTPUT" PRECISION ="6" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="date/time" DEFAULTVALUE ="ERROR(&apos;transformation error&apos;)" DESCRIPTION ="" EXPRESSION ="v_PERIOD_END_DT" EXPRESSIONTYPE ="GENERAL" NAME ="PERIOD_END_DT" PICTURETEXT ="" PORTTYPE ="OUTPUT" PRECISION ="29" SCALE ="9"/>
            <TRANSFORMFIELD DATATYPE ="date/time" DEFAULTVALUE ="ERROR(&apos;transformation error&apos;)" DESCRIPTION ="" EXPRESSION ="v_HOLD_QUE_DT" EXPRESSIONTYPE ="GENERAL" NAME ="HOLD_QUE_DT" PICTURETEXT ="" PORTTYPE ="OUTPUT" PRECISION ="29" SCALE ="9"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="This is the Period ID value that exists on the claim when this mapplet is used." NAME ="prev_PRD_ID" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="6" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="This is the Period ID value that exists on the claim when this mapplet is used.&#xD;&#xA;If the previous value IsNull, set the value to &apos;NULL&apos; for comparison purposes." EXPRESSION ="IIF(IsNull(prev_PRD_ID),&apos;NULL&apos;,prev_PRD_ID)" EXPRESSIONTYPE ="GENERAL" NAME ="v_prev_PRD_ID" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="6" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="small integer" DEFAULTVALUE ="" DESCRIPTION ="This is the Accelerated Invoicing CLM_PREVW_IND value that exists on the claim when this mapplet is used." NAME ="prev_CLM_PREVW_IND_in" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="5" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="small integer" DEFAULTVALUE ="" DESCRIPTION ="This variable port will use the three rules for setting the CLM_PREVW_IND - &#xD;&#xA;-Is newly assigned period HOLD?  CLM_PREVW_IND = 0 always&#xD;&#xA;-Is newly assigned period the same as the previous period?  CLM_PREV_IND = prev_CLM_PREVW_IND&#xD;&#xA;-If previous two are FALSE, then do a lookup on TCLM_PREVW_RULE to get the indicator." EXPRESSION ="IIF(v_PRD_ID=&apos;HOLD&apos;,0&#xD;&#xA;,IIF(in_PRD_ID=prev_PRD_ID,prev_CLM_PREVW_IND_in&#xD;&#xA;--,:LKP.LKPTRANS(in_MODEL_TYP_CD)))&#xD;&#xA;&#xD;&#xA;,:LKP.LKP_TCLM_PREVW_RULE(in_PRD_ID,in_MODEL_TYP_CD)))&#xD;&#xA;--,2))" EXPRESSIONTYPE ="GENERAL" NAME ="v_CLM_PREVW_IND" PICTURETEXT ="" PORTTYPE ="LOCAL VARIABLE" PRECISION ="5" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="small integer" DEFAULTVALUE ="ERROR(&apos;transformation error&apos;)" DESCRIPTION ="Check the value of the v_CLM_PREVW_IND.  If it is NULL, then a lookup was done on the TCLM_PREVW_RULE table and no row was found, so default the CLM_PREVW_IND_out to 1, on.&#xD;&#xA;If the value is not null, use that value for the CLM_PREVW_IND_out." EXPRESSION ="IIF(IsNull(v_CLM_PREVW_IND),1,v_CLM_PREVW_IND)" EXPRESSIONTYPE ="GENERAL" NAME ="CLM_PREVW_IND_out" PICTURETEXT ="" PORTTYPE ="OUTPUT" PRECISION ="5" SCALE ="0"/>
            <TRANSFORMFIELD DATATYPE ="string" DEFAULTVALUE ="" DESCRIPTION ="ITPR008033 had to add this port as an output in the LKP_RCIT_LOCK_CNTL so it was available in the next expression." NAME ="in_MODEL_TYP_CD" PICTURETEXT ="" PORTTYPE ="INPUT" PRECISION ="1" SCALE ="0"/>
            <TABLEATTRIBUTE NAME ="Tracing Level" VALUE ="Normal"/>
        </TRANSFORMATION>
