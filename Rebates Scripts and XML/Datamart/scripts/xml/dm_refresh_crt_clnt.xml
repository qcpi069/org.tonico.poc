<!-- REFRESH crt_clnt  
  --> 
<sqml>
  <property name="TIMEOUT" value="0" /> 
  <datasources resource="datasource.xml" /> 
  <properties resource="datamart.properties" /> 
<command type="java">
<![CDATA[ 
        mailto=script.getProperty("mailto");
        cli_schema=script.getProperty("cli_schema");        
	

  ]]> 
  </command>
<command type="try">
<try>
<!-- *************** crt_clnt ********************* 
  --> 
<command type="run" datasource="gdx" rowName="count1">
<![CDATA[ 
					SELECT COUNT(*) FROM ${cli_schema}.crt_clnt
				

  ]]> 
  </command>
<!-- Copy crt_clnt info from GDX to RPSDM. 
  --> 
<command type="copy" batch="1000">
<source type="sql" datasource="gdx">
<![CDATA[ 
					SELECT 
                                          RBAT_ID,
                                          MSTR_CLNT_ID,
                                          AP_CO_CD,
                                          PMT_HOLD_CD,
                                          CLNT_TYP_CD,
                                          PMT_TYP_CD,
                                          RBAT_INV_EFF_DT,
                                          RBAT_INV_TERM_DT,
                                          MAR_DYSP_QTY,
                                          RBEL_CLNT_CD,
                                          PMT_ELIG_CD,
                                          INSRT_TS,
                                          INSRT_USER_ID,
                                          UPDT_TS,
                                          UPDT_USER_ID,
                                          AP_VEND_NB,
                                          AP_PMT_TYP_CD,
                                          PMT_OPT_CD,
                                          CLNT_NM,
                                          RBAT_CLSTR_NM,
                                          PREV_RBAT_ID ,
                                          BUS_SEG_CD,
                                          RECN_EXCP_IN
					FROM ${cli_schema}.crt_clnt 
				

  ]]> 
  </source>
<target type="sql" datasource="datamart" transaction="true" batch="1000" reportInterval="1000" queryTimeout="${TIMEOUT}">
<![CDATA[ 
					INSERT INTO ${cli_schema}.crt_clnt
                                         (RBAT_ID,
                                          MSTR_CLNT_ID,
                                          AP_CO_CD,
                                          PMT_HOLD_CD,
                                          CLNT_TYP_CD,
                                          PMT_TYP_CD,
                                          RBAT_INV_EFF_DT,
                                          RBAT_INV_TERM_DT,
                                          MAR_DYSP_QTY,
                                          RBEL_CLNT_CD,
                                          PMT_ELIG_CD,
                                          INSRT_TS,
                                          INSRT_USER_ID,
                                          UPDT_TS,
                                          UPDT_USER_ID,
                                          AP_VEND_NB,
                                          AP_PMT_TYP_CD,
                                          PMT_OPT_CD,
                                          CLNT_NM,
                                          RBAT_CLSTR_NM,
                                          PREV_RBAT_ID ,
                                          BUS_SEG_CD,
                                          RECN_EXCP_IN
					  ) 
					VALUES (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)
				

  ]]> 
  </target>
  </command>
<!-- compare totals 
  --> 
<command type="run" datasource="datamart" rowName="count2">
<![CDATA[ 
					SELECT COUNT(*) FROM ${cli_schema}.crt_clnt
				

  ]]> 
  </command>
  <command type="java">count1 = script.getProperty("count1.1").intValue(); count2 = script.getProperty("count2.1").intValue(); logger.warn("GDX crt_clnt COUNT = " + count1); logger.warn("RPSDM crt_clnt COUNT = " + count2); if (count1 != count2) { logger.error(" crt_clnt counts do NOT match count1=" + count1 + " count2=" + count2); throw new Exception(" crt_clnt counts do NOT match count1=" + count1 + " count2=" + count2); }</command> 
  </try>
<catch>
<!-- Send an email on error 
  --> 
<command type="mail" mailTo="${mailto}" mailFrom="rpsadm@caremark.com" subject="[ERROR] GDX -> DM REFESH crt_clnt" contentType="text/html" throwsException="false">
<![CDATA[ 
				<html>
				<head>
				<title>[ERROR] GDX -> DM REFRESH crt_clnt</title>
				</head>
				<body>
			
				<h3><u>[ERROR] GDX -> DM REFRESH crt_clnt </u></h3>
			
				<table cellpadding="2">
					<tr><td><b>Host:</b></td><td>${localhost}<td></tr>
					<tr><td><b>Started:</b></td><td>${startTime}<td></tr>
					<tr><td><b>Script:</b></td><td>${scriptName}<td></tr>
					#if (${system.getProperty('logfile')})
						<tr>
							<td><b>Log File:</b></td>
							<td>
								${system.getProperty('logfile')}
							<td>
						</tr>
					#end
				</table>

				<br>			
				<br>
				
				#if (${script.getLastStatement()})
					<h3><u>Last Statement</u></h3>
					<font style="color: blue; font-size: small">	
					${script.getLastStatement()}
					</font>
				
					<br>
					<br>
				#end
			
				<h3><u>Stack Trace</u></h3>		
				<pre style="color: red; font-size: small;">${stackTrace}</pre>
				</body>
				</html>
			

  ]]> 
  </command>
  </catch>
  </command>
  </sqml>
