
<!-- REFRESH ENROLMENT_QUARTERLY_UTIL -->

<sqml>
	<property name="TIMEOUT" value="0"/>
	<datasources resource="datasource.xml"/>
	<properties resource="datamart.properties"/>

    <command type="java"><![CDATA[
        mailto=script.getProperty("mailto");
        cli_schema=script.getProperty("cli_schema");        
	]]></command>
		       
	<command type="try">
		<try>
            <!-- *************** ENROLMENT_QUARTERLY_UTIL ********************* -->
			<command type="run" datasource="rebates" rowName="count1">
				<![CDATA[
					SELECT COUNT(*) FROM ${cli_schema}.enrollment_quarterly_util
				]]>
			</command>
			<!-- Copy enrolment_quarterly_util info from Oracle to DB2. -->
			<command type="copy" batch="500">
				<source type="sql" datasource="rebates"><![CDATA[
					SELECT
					    rpt_grp_nbr
					    ,rpt_grp_nm
					    ,rbat_inv_eff_dt
					    ,rbat_inv_term_dt
					    ,frmly_id
					    ,frmly_nm
					    ,prcsr_typ
					    ,rbat_typ
					    ,rac
					    ,qtr_vl
					    ,yr_vl
					    ,model_typ_cd
					    ,ap_compny_cd
					    ,basic_month1
					    ,basic_month2
					    ,basic_month3
					    ,perf_month1
					    ,perf_month2
					    ,perf_month3
					    ,perf_inct_month1
					    ,perf_inct_month2
					    ,perf_inct_month3
					    ,perf_inct_plus_month1
					    ,perf_inct_plus_month2
					    ,perf_inct_plus_month3
					    ,closed_month1
					    ,closed_month2
					    ,closed_month3
					    ,total_month1
					    ,total_month2
					    ,total_month3
					    ,total_avg
					FROM ${cli_schema}.enrollment_quarterly_util
				]]></source>
			<target type= "queue" threads="2" >  
				<target
					type             = "sql"
					datasource       = "datamart"
					transaction      = "true"
					batch            = "500"
					reportInterval   = "500"
					queryTimeout     = "${TIMEOUT}"
				><![CDATA[
					INSERT INTO ${cli_schema}.enrollment_quarterly_util
					(rpt_grp_nbr
					,rpt_grp_nm
					,rbat_inv_eff_dt
					,rbat_inv_term_dt
					,frmly_id
					,frmly_nm
					,prcsr_typ
					,rbat_typ
					,rac
					,qtr_vl
					,yr_vl
					,model_typ_cd
					,ap_compny_cd
					,basic_month1
					,basic_month2
					,basic_month3
					,perf_month1
					,perf_month2
					,perf_month3
					,perf_inct_month1
					,perf_inct_month2
					,perf_inct_month3
					,perf_inct_plus_month1
					,perf_inct_plus_month2
					,perf_inct_plus_month3
					,closed_month1
					,closed_month2
					,closed_month3
					,total_month1
					,total_month2
					,total_month3
					,total_avg)
					VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)
				]]></target>
			 </target>
			</command>
	        <!-- compare totals -->
			<command type="run" datasource="datamart" rowName="count2">
				<![CDATA[
					SELECT COUNT(*) FROM ${cli_schema}.enrollment_quarterly_util
				]]>
			</command>
			<command type="java">
				count1 = script.getProperty("count1.1").intValue();
				count2 = script.getProperty("count2.1").intValue();
				logger.warn("Silver RBATE_REG.ENROLLMENT_QUARTERLY_UTIL COUNT = " + count1);
				logger.warn("RPS RBATE_REG.ENROLLMENT_QUARTERLY_UTIL COUNT = " + count2);
				if (count1 != count2) {
					logger.error(" ENROLLMENT_QUARTERLY_UTIL counts do NOT match count1=" + count1 + "  count2=" + count2);
                    throw new Exception(" ENROLLMENT_QUARTERLY_UTIL counts do NOT match count1=" + count1 + " count2=" + count2);
				} 
			</command> 
				 
		</try>
		<catch>
			<!-- Send an email on error -->
			<command
				type="mail"
				mailTo="${mailto}"
				mailFrom="RPDM_Client_Refresh"
				subject="[ERROR] REFESH CLIENT - ENROLLMENT QUARTERLY UTIL "
				contentType="text/html"
				throwsException="false"
			><![CDATA[
				<html>
				<head>
				<title>[ERROR] REFRESH CLIENT - ENROLLMENT_QUARTERLY_UTIL</title>
				</head>
				<body>
			
				<h3><u>[ERROR] REFRESH CLIENT - ENROLLMENT_QUARTERLY_UTIL</u></h3>
			
				<table cellpadding="2">
					<tr><td><b>Host:</b></td><td>${localhost}<td></tr>
					<tr><td><b>Started:</b></td><td>${startTime}<td></tr>
					<tr><td><b>Script:</b></td><td>${scriptName}<td></tr>
					#if (${system.getProperty('logfile')})
						<tr>
							<td><b>Log File:</b></td>
							<td>
								${system.getProperty('logfile')}
							<td>
						</tr>
					#end
				</table>

				<br>			
				<br>
				
				#if (${script.getLastStatement()})
					<h3><u>Last Statement</u></h3>
					<font style="color: blue; font-size: small">	
					${script.getLastStatement()}
					</font>
				
					<br>
					<br>
				#end
			
				<h3><u>Stack Trace</u></h3>		
				<pre style="color: red; font-size: small;">${stackTrace}</pre>
				</body>
				</html>
			]]></command>
		</catch>
	</command>
</sqml>
