<!-- REFESH MAINFRAME PAYMENTS TABLE kscc030_clnt  
  --> 
<sqml>
  <property name="TIMEOUT" value="0" /> 
  <datasources resource="datasource.xml" /> 
  <properties resource="datamart.properties" /> 
<command type="java">
<![CDATA[ 
        mailto=script.getProperty("mailto");
        dmt_schema=script.getProperty("cli_schema");
        pmt_schema=script.getProperty("pmt_schema");
	

  ]]> 
  </command>
<command type="try">
<try>
<!-- delete from DB2. 
  --> 
<command type="run" datasource="db2p">
<![CDATA[ 
					DELETE FROM ${pmt_schema}.kscc030_clnt
				

  ]]> 
  </command>
<!-- Copy from datamart UDB to mainframe DB2. 
  --> 
<command type="copy" batch="1000">
<source type="sql" datasource="datamart">
<![CDATA[ 
    SELECT RBAT_ID,
                                          MSTR_CLNT_ID,
                                          AP_CO_CD,
                                          PMT_HOLD_CD,
                                          CLNT_TYP_CD,
                                          PMT_TYP_CD,
                                          RBAT_INV_EFF_DT,
                                          RBAT_INV_TERM_DT,
                                          MAR_DYSP_QTY,
                                          RBEL_CLNT_CD,
                                          PMT_ELIG_CD,
                                          INSRT_TS,
                                          INSRT_USER_ID,
                                          UPDT_TS,
                                          UPDT_USER_ID,
                                          AP_VEND_NB,
                                          AP_PMT_TYP_CD,
                                          PMT_OPT_CD,
                                          CLNT_NM,
                                          RBAT_CLSTR_NM,
                                          PREV_RBAT_ID,
                                          BUS_SEG_CD,
                                          RECN_EXCP_IN
    FROM  ${cli_schema}.crt_clnt

				

  ]]> 
  </source>
<target type="sql" datasource="db2p" transaction="true" batch="1000" reportInterval="1000" queryTimeout="${TIMEOUT}">
<![CDATA[ 
					INSERT INTO ${pmt_schema}.kscc030_clnt
                    			( RBAT_ID,
                                          MSTR_CLNT_ID,
                                          AP_CO_CD,
                                          PMT_HOLD_CD,
                                          CLNT_TYP_CD,
                                          PMT_TYP_CD,
                                          RBAT_INV_EFF_DT,
                                          RBAT_INV_TERM_DT,
                                          MAR_DYSP_QTY,
                                          RBEL_CLNT_CD,
                                          PMT_ELIG_CD,
                                          INSRT_TS,
                                          INSRT_USER_ID,
                                          UPDT_TS,
                                          UPDT_USER_ID,
                                          AP_VEND_NB,
                                          AP_PMT_TYP_CD,
                                          PMT_OPT_CD,
                                          CLNT_NM,
                                          RBAT_CLSTR_NM,
                                          PREV_RBAT_ID,
                                          BUS_SEG_CD,
                                          RECN_EXCP_IN  
					)
					VALUES (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)
				

  ]]> 
  </target>
  </command>
<!-- compare totals 
  --> 
<command type="run" datasource="db2p" rowName="count1">
<![CDATA[ 
					SELECT COUNT(*) FROM ${pmt_schema}.kscc030_clnt
				

  ]]> 
  </command>
<command type="run" datasource="datamart" rowName="count2">
<![CDATA[ 
					SELECT COUNT(*) FROM ${cli_schema}.crt_clnt
				

  ]]> 
  </command>
  <command type="java">count1 = script.getProperty("count1.1").intValue(); count2 = script.getProperty("count2.1").intValue(); if (count1 != count2) { logger.error(" kscc030 counts do NOT match count1=" + count1 + " count2=" + count2); throw new Exception(" kscc030 counts do NOT match count1=" + count1 + " count2=" + count2); } System.out.println(" *********************************************** "); System.out.println(" * Rows refreshed into kscc030 is " + count1 + ". "); System.out.println(" *********************************************** ");</command> 
  </try>
<catch>
<!-- Send an email on error 
  --> 
<command type="mail" mailTo="${mailto}" mailFrom="rpsadm@caremark.com" subject="[ERROR] MF REFESH kscc030" contentType="text/html" throwsException="false">
<![CDATA[ 
				<html>
				<head>
				<title>[ERROR] MF REFRESH kscc030</title>
				</head>
				<body>
			
				<h3><u>[ERROR] MF REFRESH kscc030 </u></h3>
			
				<table cellpadding="2">
					<tr><td><b>Host:</b></td><td>${localhost}<td></tr>
					<tr><td><b>Started:</b></td><td>${startTime}<td></tr>
					<tr><td><b>Script:</b></td><td>${scriptName}<td></tr>
					#if (${system.getProperty('logfile')})
						<tr>
							<td><b>Log File:</b></td>
							<td>
								${system.getProperty('logfile')}
							<td>
						</tr>
					#end
				</table>

				<br>			
				<br>
				
				#if (${script.getLastStatement()})
					<h3><u>Last Statement</u></h3>
					<font style="color: blue; font-size: small">	
					${script.getLastStatement()}
					</font>
				
					<br>
					<br>
				#end
			
				<h3><u>Stack Trace</u></h3>		
				<pre style="color: red; font-size: small;">${stackTrace}</pre>
				</body>
				</html>
			

  ]]> 
  </command>
  </catch>
  </command>
  </sqml>
