
<!-- REFRESH KSZ6005_PCNTL_TRK  -->
<!-- 11/04/2010  Cash Recon change - added fields   -->

<sqml>
	<property name="TIMEOUT" value="0"/>
	<datasources resource="datasource.xml"/>
	<properties resource="datamart.properties"/>

    <command type="java"><![CDATA[
        mailto=script.getProperty("mailto");
        dmt_schema=script.getProperty("dmt_schema");
		pmt_schema=script.getProperty("pmt_schema");
	]]></command>
		       
	<command type="try">
		<try>

		  <!-- delete from UDB (except some type 15's hand entered from vsam file -->
			<command type="run" datasource="datamart">
				<![CDATA[
					DELETE FROM ${dmt_schema}.KSZ6005_PCNTL_TRK  
					  WHERE PRCS_NB < 1142600 
				]]>
			</command>
		

            <!-- *************** PCNTL_TRK ********************* -->
			<!-- Copy info from mainframe  -->
			<command type="copy" batch="500">
				<source type="sql" datasource="datamart"><![CDATA[
					SELECT 
                       PRCS_NB,
					   PRCS_TYP_CD,
					   PRCS_FILE_NM,
					   PRCS_FILE_CRTE_DT,
					   PRCS_STAT_CD,
					   REC_CNT_VL, 
					   PRD_TYP_CD, 
					   PRD_DT, 
					   PRCS_BEG_TS,
					   PRCS_END_TS					 
						,PRCS_CO_ID
						,PRCS_PMT_SCHD_CD
						,PRCS_REF_TRAN_ID
					FROM ${pmt_schema}.KSZ6005_PCNTL_TRK  
					 WHERE PRCS_STAT_CD IN ('C', 'W', 'A', 'O') 
						ORDER BY 1
				]]></source>
				<target
					type             = "sql"
					datasource       = "datamart"
					transaction      = "true"
					batch            = "500"
					reportInterval   = "500"
					queryTimeout     = "${TIMEOUT}"
				><![CDATA[
					INSERT INTO ${dmt_schema}.KSZ6005_PCNTL_TRK
					 (PRCS_NB,
					   PRCS_TYP_CD,
					   PRCS_FILE_NM,
					   PRCS_FILE_CRTE_DT,
					   PRCS_STAT_CD,
					   REC_CNT_VL, 
					   PRD_TYP_CD, 
					   PRD_DT, 
					   PRCS_BEG_TS,
					   PRCS_END_TS	
						,PRCS_CO_ID
						,PRCS_PMT_SCHD_CD
						,PRCS_REF_TRAN_ID
					  ) 
					VALUES ( ?, ?, ?, ?, ?, ?, ?, ?, ?, ?,?,?,? )
				]]></target>
            
			</command>

			
	        <!-- (dont) compare totals -->
			<!-- 
			<command type="run" datasource="db2p" rowName="count1">
				<![CDATA[
					SELECT COUNT(*) FROM ${pmt_schema}.KSZ6005_PCNTL_TRK
                ]]>
			</command>
			<command type="run" datasource="datamart" rowName="count2">
				<![CDATA[
					SELECT COUNT(*) FROM ${dmt_schema}.KSZ6005_PCNTL_TRK
                ]]>
			</command>
			<command type="java">
				count1 = script.getProperty("count1.1").intValue();
				count2 = script.getProperty("count2.1").intValue();
				logger.warn("mainframe KSZ6005 COUNT = " + count1);
				logger.warn("RPS KSZ6005  COUNT = " + count2);
				if (count1 != count2) {
					logger.error(" KSZ6005 counts do NOT match count1=" + count1 + "  count2=" + count2);
                    throw new Exception(" KSZ6005 counts do NOT match count1=" + count1 + " count2=" + count2);
				} 
			</command>
			-->
				 
		</try>
		<catch>
			<!-- Send an email on error -->
			<command
				type="mail"
				mailTo="${mailto}"
				mailFrom="RPDM_Refresh"
				subject="[ERROR] REFESH - KSZ6005 "
				contentType="text/html"
				throwsException="false"
			><![CDATA[
				<html>
				<head>
				<title>[ERROR] REFESH - KSZ6005</title>
				</head>
				<body>
			
				<h3><u>[ERROR] REFESH - KSZ6005 </u></h3>
			
				<table cellpadding="2">
					<tr><td><b>Host:</b></td><td>${localhost}<td></tr>
					<tr><td><b>Started:</b></td><td>${startTime}<td></tr>
					<tr><td><b>Script:</b></td><td>${scriptName}<td></tr>
					#if (${system.getProperty('logfile')})
						<tr>
							<td><b>Log File:</b></td>
							<td>
								${system.getProperty('logfile')}
							<td>
						</tr>
					#end
				</table>

				<br>			
				<br>
				
				#if (${script.getLastStatement()})
					<h3><u>Last Statement</u></h3>
					<font style="color: blue; font-size: small">	
					${script.getLastStatement()}
					</font>
				
					<br>
					<br>
				#end
			
				<h3><u>Stack Trace</u></h3>		
				<pre style="color: red; font-size: small;">${stackTrace}</pre>
				</body>
				</html>
			]]></command>
		</catch>
	</command>
</sqml>
