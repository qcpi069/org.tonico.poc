			name="newCtidName"
			position="8"
			dataType="string"
			trim="true"
		/>
		<field
			name="newAssoEffectiveDateStr"
			position="9"
			dataType="string"
			trim="true"
		/>
		<field
			name="newAssoTerminationDateStr"
			position="10"
			dataType="string"
			trim="true"
		/>
		
	</record>
</parsers>
