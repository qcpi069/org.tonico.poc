   	<property name="VEND_ID" column="VEND_ID" type="int" />
  	<property name="BE_ID" column="BE_ID" type="int"/>
  	<property name="PRD_ID" column="PRD_ID" type="string"/>
  	<property name="FILE_USE_CD" column="FILE_USE_CD" type="short"/>
  	<property name="FILE_SEQ_NB" column="FILE_SEQ_NB" type="short"/>
  	<property name="COL_MTCH_SPEC_CD" column="COL_MTCH_SPEC_CD" type="int"/> 
  	<property name="FILE_NM" column="FILE_NM" type="string"/>   	
  	<property name="BFF_FILE_STAT_CD" column="BFF_FILE_STAT_CD" type="short"/>
  </class>  
</hibernate-mapping>
