<formatters>
	<record type="posFeedTrailer">	
		<literal>T</literal>
		<format
			name="dateTime"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="fileName"
			align="left"
			minLength="40"
			maxLength="40"
		/>
		<format
			name="recordCount"
			align="right"
			dataType="decimal"
			format="0000000000"
			minLength="10"
			maxLength="10"
		/>
		<literal>                                       					                     </literal> 
	</record>
	<record type="posFeed">
		<format
			name="RECORD_ACTION_CODE"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="EXTL_LVL1_ID"
			align="right"
			minLength="20"
			maxLength="20"
			
		/>
		<format
			name="EXTL_LVL2_ID"
			align="left"
			minLength="20"
			maxLength="20"
		/>
		<format
			name="EXTL_LVL3_ID"
			align="left"
			minLength="20"
			maxLength="20"
		/>
		<format
			name="EXTL_LVL4_ID"
			align="left"
			minLength="20"
			maxLength="20"
		/>
		<format
			name="EXTL_LVL5_ID"
			align="right"
			minLength="20"
			maxLength="20"
		/>
		<format
			name="EFF_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="TERM_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="PLATFORM_ID"
			align="left"
			minLength="5"
			maxLength="5"
		/>
		<format
			name="SPECLTY_EXCL"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="POS_INDICATOR"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="CACL_SHR"
			align="right"
			dataType="decimal"
			format="00000"
			minLength="5"
			maxLength="5"
			script="value * 100.0"
		/>
		<format
			name="MODULATOR"
			align="right"
			dataType="decimal"
			format="00000"
			minLength="5"
			maxLength="5"
			script="value * 100.0"
		/>
		<!-- 20 filler spaces -->
		<literal>                   </literal>
	</record>
</formatters>
