<formatters>
	
	<record type="rateTrailer">	
		<literal>T</literal>
		<format
			name="dateTime"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<literal>MFG_RATE                                </literal>
		<format
			name="recordCount"
			align="right"
			dataType="decimal"
			format="0000000000"
			minLength="10"
			maxLength="10"
		/>
		<literal>                                       </literal>
	</record>
	
	<record type="rate">
		<format
			name="CHANGE_TYPE"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="PICO_ID"
			align="right"
			dataType="decimal"
			format="00000"
			minLength="5"
			maxLength="5"
		/>
		<format
			name="FRMLY_ID"
			align="left"
			minLength="10"
			maxLength="10"
		/>
		<format
			name="FRMLY_SRC_CD"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="NDC11_ID"
			align="left"
			minLength="11"
			maxLength="11"
		/>
		<format
			name="NHU_TYPE"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="EFF_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="TERM_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="DRUG_PRC_SRC_CD"
			align="right"
			dataType="decimal"
			format="00"
			minLength="2"
			maxLength="2"
		/>
		<format
			name="DRUG_PRC_TYP_CD"
			align="right"
			dataType="decimal"
			format="00"
			minLength="2"
			maxLength="2"
		/>
		<format
			name="RATE_QLFR_CD"
			align="left"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="MAIL_RATE_T2_AMT"
			align="right"
			dataType="decimal"
			format="0000000"
			minLength="7"
			maxLength="7"
			script="value * 100.0"
		/>
		<format
			name="MAIL_RATE_T3_AMT"
			align="right"
			dataType="decimal"
			format="0000000"
			minLength="7"
			maxLength="7"
			script="value * 100.0"
		/>
		<format
			name="RTL_RATE_T2_AMT"
			align="right"
			dataType="decimal"
			format="0000000"
			minLength="7"
			maxLength="7"
			script="value * 100.0"
		/>
		<format
			name="RTL_RATE_T3_AMT"
			align="right"
			dataType="decimal"
			format="0000000"
			minLength="7"
			maxLength="7"
			script="value * 100.0"
		/>
		<format
			name="EXCL_MAIL_CD"
			align="left"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="EXCL_THER_CD"
			align="left"
			minLength="1"
			maxLength="1"
		/>
		<!-- 16 filler spaces -->
		<literal>                </literal>
	</record>
	
	<record type="pharmacyTrailer">	
		<literal>T</literal>
		<format
			name="dateTime"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<literal>MFG_PMCY_EXCL                           </literal>
		<format
			name="recordCount"
			align="right"
			dataType="decimal"
			format="0000000000"
			minLength="10"
			maxLength="10"
		/>
	</record>
	
	<record type="pharmacy">
		<format
			name="CHANGE_TYPE"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="PICO_ID"
			align="right"
			dataType="decimal"
			format="00000"
			minLength="5"
			maxLength="5"
		/>
		<format
			name="EFF_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="TERM_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="PMCY_ID"
			align="left"
			minLength="7"
			maxLength="7"
		/>
		<format
			name="PGM_CD"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<literal>                           </literal>
	</record>
	
	
	<record type="stateTrailer">	
		<literal>T</literal>
		<format
			name="dateTime"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<literal>MFG_ST_EXCL                             </literal>
		<format
			name="recordCount"
			align="right"
			dataType="decimal"
			format="0000000000"
			minLength="10"
			maxLength="10"
		/>
	</record>
	
	<record type="state">
		<format
			name="CHANGE_TYPE"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<format
			name="PICO_ID"
			align="right"
			dataType="decimal"
			format="00000"
			minLength="5"
			maxLength="5"
		/>
		<format
			name="EFF_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="TERM_DT"
			align="left"
			dataType="date"
			format="MM/dd/yyyy"
		/>
		<format
			name="ST_CD"
			align="left"
			minLength="2"
			maxLength="2"
		/>
		<format
			name="PGM_CD"
			align="right"
			minLength="1"
			maxLength="1"
		/>
		<literal>                                </literal>
	</record>
	
</formatters>
