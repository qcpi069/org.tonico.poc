<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client split conversion process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.XREFLoadMain">	
		<property name="service">
    			<ref bean="service"/>
    		</property>		
		
		<property name="xrefLoadEmailTemplate">
    			<value>XREFLoadEmailTemplate.txt</value>
    		</property>	
		<property name="mailFrom">
    			<value>XREFManualLoad@caremark.com</value>
    		</property>	
		<property name="mailTo">
    			<value>gdxitd@caremark.com</value>
    		</property>	
		<!--
		<property name="mailTo">
    			<value>shirley.han@caremark.com</value>
    		</property>	
			-->
    </bean>
	<bean id="service" class="com.caremark.rebates.registration.batch.service.XREFLoadService">	
		<property name="dataSource">
            <ref bean="rbateRegDataSource"/>
        </property>	
		<property name="transactionTemplate">
            <ref bean="transactionTemplate"/>
        </property> 
			<property name="archivePrefix">
    			<value>XREF_LOAD</value>
    		</property>		
		<property name="inputDir">
    			<value>data/incoming</value>
    		</property>	
		<property name="dataFileName">    		
			<value>xref.csv</value>
    		</property>	
		<property name="archiveDir">
    			<value>data/completed</value>
    		</property>	
		<property name="errorDir">
    			<value>data/error</value>
    		</property>	
		<property name="logDir">
    			<value>logs</value>
    		</property>	
		<property name="carrierDAO">
    			<ref bean="carrierDAO"/>
    		</property>	
	<property name="cagClientAssocDAO">
    			<ref bean="cagClientAssocDAO"/>
    		</property>	
		<property name="cagRacAssocDAO">
    			<ref bean="cagRacAssocDAO"/>
    		</property>	
		<property name="cagLcmAssocDAO">
    			<ref bean="cagLcmAssocDAO"/>
    		</property>	
		
		<property name="clientDAO">
    			<ref bean="clientDAO"/>
    		</property>		
		
	<property name="cagAssocAuditDAO">
    			<ref bean="cagAssocAuditDAO"/>
    		</property>	
		<property name="systemCodeDAO">
    			<ref bean="systemCodeDAO"/>
    		</property>	
		
		<property name="cagDAO">
    			<ref bean="cagDAO"/>
    		</property>	
			
    </bean>
	
	
		<!--
    <bean id="messageSource" class="org.springframework.context.support.ResourceBundleMessageSource">
        <property name="basename"><value>messages</value></property>
    </bean>	
-->
</beans>
