<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client APC Status Load process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.ClientRegPricingConversionMain">	
		<property name="service">
    		<ref bean="clientRegPricingConversionService"/>
    	</property>		
    </bean>
	<bean id="clientRegPricingConversionService" class="com.caremark.rebates.registration.batch.service.ClientRegPricingConversionService">	
		
		<property name="dataSource">
            <ref bean="rbateRegDataSource"/>
        </property>	
		
		<property name="transactionTemplate">
        	<ref bean="transactionTemplate"/>
        </property> 
		
		<property name="clientRegPricingConversionDAO">
    		<ref bean="clientRegPricingConversionDAO"/>
    	</property>	
		
		<property name="logDir">
    			<value>logs</value>
    	</property>
    	
    	<property name="fileNameCSV">
    			<value>conversion.csv</value>
    	</property>	
    	
    	<property name="fileNameText">
    			<value>conversion.txt</value>
    	</property>	
    </bean>	
	
</beans>
