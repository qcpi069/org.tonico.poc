<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client split conversion process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.ClientHierarchyLoadMain">	
		<property name="service">
    			<ref bean="clientHierarchyLoadService"/>
    		</property>		
    </bean>

	<bean id="clientHierarchyLoadService" class="com.caremark.rebates.registration.batch.service.ClientHierarchyLoadService">	
		<property name="dataSource">
            <ref bean="rbateRegDataSource"/>
        </property>	
		<property name="transactionTemplate">
            <ref bean="transactionTemplate"/>
        </property> 
			<property name="archivePrefix">
    			<value>CLIENT_HIERARCHY_LOAD</value>
    		</property>		
		<property name="inputDir">
    			<value>data/incoming</value>
    		</property>	
		<property name="dataFileName">    		
			<value>client.txt</value>
    	</property>	
    	<property name="masterFileName">    		
			<value>master.txt</value>
    	</property>	
    		
		<property name="archiveDir">
    			<value>data/completed</value>
    		</property>	
		<property name="errorDir">
    			<value>data/error</value>
    		</property>	
		<property name="logDir">
    			<value>logs</value>
    		</property>	
    	<property name="masterClientDAO">
    			<ref bean="masterClientDAO"/>
    		</property>	
    	<property name="racDspnFclyAssocDAO">
    			<ref bean="racDspnFclyAssocDAO"/>
    		</property>	
    	<property name="racClientAssocDAO">
    			<ref bean="racClientAssocDAO"/>
    		</property>	
    	<property name="rptClientDAO">
    			<ref bean="rptClientDAO"/>
    		</property>	
		<property name="commentsDAO">
    			<ref bean="commentsDAO"/>
    		</property>	    		
    	<property name="pcsContractDAO">
    			<ref bean="pcsContractDAO"/>
    		</property>	
    	<property name="durDAO">
    			<ref bean="durDAO"/>
    		</property>	
		<property name="rebateInvoiceDAO">
    			<ref bean="rebateInvoiceDAO"/> 
    	</property>
    	<property name="clientModelAssocDAO">
    			<ref bean="clientModelAssocDAO"/>
    		</property>	
    	<property name="clientMasterAssocDAO">
    			<ref bean="clientMasterAssocDAO"/>
    		</property>	
		<property name="rebateDAO">
    			<ref bean="rebateDAO"/>
    		</property>	
		<property name="racDAO">
    			<ref bean="racDAO"/>
    	</property>	
		<property name="clientDAO">
    			<ref bean="clientDAO"/>
    	</property>		
    	<property name="systemCodeDAO">
    		<ref bean="systemCodeDAO"/>
    	</property>		
		
    </bean>
	
	
		<!--
    <bean id="messageSource" class="org.springframework.context.support.ResourceBundleMessageSource">
        <property name="basename"><value>messages</value></property>
    </bean>	
-->
</beans>
