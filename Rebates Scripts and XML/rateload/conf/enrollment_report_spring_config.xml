<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client split conversion process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	<!--  transaction manager -->
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.EnrollmentReportMain">	
		<property name="service">
    		<ref bean="enrollmentReportService"/>
    	</property>		
    </bean>
	<bean id="enrollmentReportService" class="com.caremark.rebates.registration.batch.service.EnrollmentReportService">	
		
		<property name="dataSource">
            <ref bean="rbateRegDataSource"/>
        </property>	
		
		<property name="transactionTemplate">
            <ref bean="transactionTemplate"/>
        </property> 
		
		<property name="finalFrmlyLcmUtilDao">
    		<ref bean="finalFrmlyLcmUtilDao"/>
    	</property>	
		
		<property name="enrollmentReportWorkDao">
    		<ref bean="enrollmentReportWorkDao"/>
    	</property>	
    	
    	<property name="enrollmentReportAuditDao">
    		<ref bean="enrollmentReportAuditDao"/>
    	</property>	
		
		<property name="systemCodesDao">
    		<ref bean="systemCodeDAO"/>
    	</property>	
    </bean>	
	
</beans>
