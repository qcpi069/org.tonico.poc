<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client split conversion process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	<!--  transaction manager -->
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.CAGClientLoadMain">	
		<property name="service">
    			<ref bean="cagClientLoadService"/>
    		</property>		
    </bean>
	<bean id="cagClientLoadService" class="com.caremark.rebates.registration.batch.service.CAGClientLoadService">	
		
		<property name="dataSource">
			<ref bean="rbateRegDataSource"/>
		</property>
	
		
		<property name="transactionTemplate">
        	    	<ref bean="transactionTemplate"/>
        </property> 
		
		
		<property name="archivePrefix">
    			<value>CAG_ONE_TIME_LOAD_ARCHIVE</value>
    	</property>	
		
		<property name="inputDir">
    			<value>data/incoming</value>
    		</property>	
		
		<property name="dataFileName">
    			<value>cag_client.dat</value>
    	</property>	
		
		<property name="archiveDir">
    			<value>data/completed</value>
    	</property>	
		
		<property name="errorDir">
    			<value>data/error</value>
    	</property>	
		
		<property name="logDir">
    			<value>logs</value>
    	</property>	
		
		<property name="carrierDAO">
    			<ref bean="carrierDAO"/>
    	</property>	
		
		<property name="cagClientAssocDAO">
    			<ref bean="cagClientAssocDAO"/>
    	</property>	
		
		<property name="cagRacAssocDAO">
    			<ref bean="cagRacAssocDAO"/>
    	</property>	
		
		<property name="cagDAO">
    			<ref bean="cagDAO"/>
    	</property>
    		<property name="clientDAO">
			<ref bean="clientDAO"/>
		</property>		
    </bean>
	
</beans>
