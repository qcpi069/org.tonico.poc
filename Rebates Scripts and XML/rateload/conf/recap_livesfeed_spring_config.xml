<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
	This is the spring configuration file for discount client split conversion process
	-->																																				 
																																					 <!--  - Application context definition for "springapp" DispatcherServlet.  -->
<beans>    
	<!--  transaction manager -->
	
	<!--  the driver class -->
	<bean id="main" class="com.caremark.rebates.registration.batch.main.RECAPLivesFeedMain">	
		<property name="service">
    			<ref bean="livesFeedService"/>
    		</property>		
		<property name="recapLivesFeedEmailTemplate">
    			<value>RECAPLivesFeedEmailTemplate.txt</value>
    		</property>			
		<property name="mailFrom">
    			<value>RECAPLivesFeed@caremark.com</value>
    		</property>	
<!--
		<property name="mailTo">
    			<value>yanping.zhao@caremark.com</value>
    		</property>	
-->
		<property name="mailTo">
    			<value>gdxitd@caremark.com</value>
    		</property>	

    </bean>
	<bean id="livesFeedService" class="com.caremark.rebates.registration.batch.service.LivesFeedService">	
		<property name="dataSource">
            <ref bean="rbateRegDataSource"/>
        </property>	
		<property name="transactionTemplate">
            <ref bean="transactionTemplate"/>
        </property>
        <property name="archivePrefix">
    			<value>RECAP_LIVES_FEED</value>
    		</property>	 
		<property name="inputDir">
    			<value>data/incoming</value>
    		</property>	
		<property name="dataFileName">
			<value>recap_enrl_cag.dat</value>
    		</property>	
		<property name="triggerFileName">    		
			<value>recap_enrl_cag.trigger</value>
    		</property>	
		<property name="archiveDir">
    			<value>data/completed</value>
    		</property>	
		<property name="errorDir">
    			<value>data/error</value>
    		</property>	
		<property name="logDir">
    			<value>logs</value>
    		</property>
    	<property name="livesFeedFileParser">
    			<value>RECAPLivesFeedFileParser.xml</value>
    	</property>	
		<property name="extlHrcyDAO">
    			<ref bean="extlHrcyDAO"/>
    		</property>	
		<property name="enrollDAO">
    			<ref bean="enrollDAO"/>
    		</property>		   	
    	<property name="medbExtlSrc">
    			<value>BALV</value>
    		</property>	
    </bean>	
	
</beans>
