			name="age"
			position="1"
			dataType="double"
			trim="true"	
		/>
		<field
			name="entryDate"
			position="2"
			dataType="date"
			format="MM/dd/yy"
			trim="true"	
		/>
	</record>
	
	<record type="body2" objectType="com.caremark.gdx.common.test.TRecord">
		
