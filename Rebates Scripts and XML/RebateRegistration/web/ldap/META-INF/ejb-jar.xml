<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>

    <enterprise-beans>

      <session>
	<ejb-name>RebatesAuthentication</ejb-name>
         <home>com.advpcs.rebates.registration.ldap.ejb.RebatesAuthenticationEJBHome</home>
         <remote>com.advpcs.rebates.registration.ldap.ejb.RebatesAuthenticationEJB</remote>
         <ejb-class>com.advpcs.rebates.registration.ldap.ejb.RebatesAuthenticationEJBBean</ejb-class>
      	<session-type>Stateless</session-type>
	<transaction-type>Container</transaction-type>

      <env-entry>
        <env-entry-name>ldap/LdapURL</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>t3://204.99.16.188:7001</env-entry-value>
      </env-entry>

      <env-entry>
        <env-entry-name>ldap/LdapInitialContextFactory</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>weblogic.jndi.WLInitialContextFactory</env-entry-value>
      </env-entry>

       <env-entry>
        <env-entry-name>ldap/LdapJndi</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>RebatesLdap</env-entry-value>
      </env-entry>
      </session>


    </enterprise-beans>
 </ejb-jar>
